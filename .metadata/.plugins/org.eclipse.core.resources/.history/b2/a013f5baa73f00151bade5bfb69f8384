import java.io.BufferedReader;
import java.io.File;
import java.io.FileNotFoundException;
import java.io.FileReader;
import java.util.Scanner;
import java.util.HashMap;

public class PFind {
	
	 /**
     * @param filePath is the path to a file, or simply the file
     * name if the file is in the same directory as me.
     * @returns the string representation of FILEPATH.
     */
    static String fileToStr(String filePath) {
        try {
            StringBuilder fileData = new StringBuilder(100000);
            BufferedReader reader = new BufferedReader(
                                    new FileReader(filePath));
            char[] buf = new char[1024];
            int numRead = 0;
            while ((numRead = reader.read(buf)) != -1) {
                String readData = String.valueOf(buf, 0, numRead);
                fileData.append(readData);
                buf = new char[1024];
            }

            reader.close();

            String returnStr = fileData.toString();
            return returnStr;
        } catch (IOException e) {
            return null;
        }
    }
            
	
	public static void main(String[] args) {
		File puzz = new File("puzzle.stuff");
		HashMap<String, Integer> coordinates = new HashMap<String, Integer>();
		try {
			Scanner sc = new Scanner(puzz);
			String pattern = "[a-zA-Z][^a-m][a-z][A-Z][a-l][mnopqr]";
			String pattern2 = "[a-zA-Z][^a-m][a-z][A-Z][s-z][mnopqr]";

			String pattern4 = "[a-zA-Z][^A-M][A-Z][a-z][A-L][MNOPQR]";
			String pattern41 = "[a-zA-Z][^A-M][A-Z][a-z][S-Z][MNOPQR]";
			int lineCount = 0;
			while (sc.hasNextLine()) {
				String s = sc.nextLine();
//				if (s.matches(pattern) || s.matches(pattern2)) {
//					coordinates.put(s, lineCount);
//				}
				if (s.matches(pattern4) || s.matches(pattern41)){
					coordinates.put(s, -lineCount);
				}
				lineCount += 1;
			}
			for (String s1: coordinates.keySet()){
				System.out.println(s1 + " " + coordinates.get(s1));
			}
		} catch (FileNotFoundException e) {

		}
	}
}